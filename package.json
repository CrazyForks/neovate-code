{
  "name": "takumi",
  "version": "0.0.4",
  "type": "module",
  "files": [
    "dist"
  ],
  "bin": {
    "takumi": "./dist/cli.mjs"
  },
  "scripts": {
    "dev": "tsx ./src/cli.ts",
    "format": "prettier --write .",
    "build": "bun build src/cli.ts --minify --outfile dist/cli.mjs --target=node",
    "release": "utools release --git-tag --github-release --changelog",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "cli",
    "takumi",
    "takumi-cli",
    "ai"
  ],
  "authors": [
    "chencheng <sorrycc@gmail.com> (https://github.com/sorrycc)"
  ],
  "license": "MIT",
  "description": "AI pair programming CLI.",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.13.10",
    "@types/yargs-parser": "^21.0.3",
    "@umijs/tools": "^0.1.34",
    "prettier": "^3.5.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.2.1",
    "@types/lodash-es": "^4.17.12",
    "ai": "^4.2.5",
    "dotenv": "^16.4.7",
    "glob": "^11.0.1",
    "inquirer": "^12.5.0",
    "lodash-es": "^4.17.21",
    "ollama-ai-provider": "^1.2.0",
    "pathe": "^2.0.3",
    "picocolors": "^1.1.1",
    "yargs-parser": "^21.1.1",
    "zod": "^3.24.2"
  }
}
